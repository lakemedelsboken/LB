	{
		"title": "Primär hypertoni och graviditet",
		"level": 4,
		"id": "i1-2_27",
		"parentId": "i1-2_26",
		"chapter": "i1-2_gyn_sjukdlmgravamning_2013fm10.html",
		"url": "/kapitel/gynekologi/sjukdomar_och_lakemedel_under_graviditet_och_amning.html",
		"type": "header",
