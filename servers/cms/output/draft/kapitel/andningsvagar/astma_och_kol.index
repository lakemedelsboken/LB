	{
		"title": "Diagnostik",
		"level": 4,
		"id": "m1_122",
		"parentId": "m1_116",
		"chapter": "m1_and_astmakol_2013fm10.html",
		"url": "/kapitel/andningsvagar/astma_och_kol.html",
