	{
		"title": "Antiöstrogener",
		"level": 4,
		"id": "j1_15",
		"parentId": "j1_12",
		"chapter": "j1_onk_farmbehmaltumor_2013fm10.html",
		"url": "/kapitel/onkologi/farmakologisk_behandling_av_maligna_tumorer.html",
		"type": "header",
